{
  "name": "fut-desktop-app-frontend",
  "productName": "FUT Desktop App",
  "version": "1.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "1. build": "ng build && yarn run build:electron:main",
    "2. start-ng": "ng serve --base-href /",
    "3. electron (2. must be running.)": "electron . --serve --dev",
    "build": "ng build && yarn run build:electron:main",
    "start-ng": "ng serve --base-href /",
    "electron": "electron . --serve --dev",
    "ng": "ng",
    "test": "ng test --codeCoverage=true --watch=true --sourceMap=true",
    "test:ci": "ng test --codeCoverage=true --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "lint:ci": "tslint --project tsconfig.json --c tslint.json 'src/**/*.ts'",
    "e2e": "ng e2e",
    "sonar-scanner": "sonar-scanner -Dsonar.projectKey=fut-desktop-app-frontend -Dsonar.sources=. -Dsonar.host.url=http://localhost:9000 -Dsonar.login=<SONAR_ID_REPLACE_THIS>",
    "ng-prod": "ng build --prod=true --sourceMap=false",
    "build:electron:main": "yarn compile && copyfiles -f package.json main.js dist && yarn run move-package-json && cd dist && yarn install --production=true && cd ..",
    "move-package-json": "cd dist && rimraf package*.json && cd .. && copyfiles -f prod-package.json dist && cd dist && rename prod-package.json package.json",
    "build:prod": "cross-env NODE_ENV='production' && yarn run ng-prod && npm run build:electron:main --asar.unpack=\"{*.jar}\"",
    "build:prod:ci": "cross-env NODE_ENV='production' && yarn run ng-prod && npm run copy:assets && npm run build:electron:main --asar.unpack=\"{*.jar}\"",
    "electron:serve": "yarn run build:electron:main && electron ./dist",
    "electron:test": "electron ./dist",
    "electron:dev": "yarn run build && electron ./dist",
    "electron:prod": "yarn run build:prod && electron ./dist",
    "electron:linux": "yarn run build:prod && node src/package.json --asar --platform=linux --arch=x64",
    "electron:windows": "yarn run build:prod && electron-builder build --windows",
    "electron:mac": "yarn run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && yarn run build",
    "start": "yarn install && electron ./app",
    "compile": "tsc -p tsconfig-serve.json",
    "copy:assets": "copyfiles -f ./src/assets/** ./dist/assets && copyfiles -f ./src/favicon.ico ./dist",
    "main & copy": "yarn compile && copyfiles -f main.js dist",
    "dist": "yarn run compile && build",
    "unpack": "asar",
    "old-electron:windows": "yarn run build:prod && node src/package.json --asar --platform=win32 --arch=ia32",
    "release": "yarn run build:prod && yarn run dist",
    "release:ci": "yarn run build:prod:ci && yarn run dist"
  },
  "private": true,
